@model AteaHomeProject.ViewModels.ConsoleMessageModel

@{
    ViewBag.Title = "Message";
}
 
    <h2>Meddelanden</h2>
    
    <table class="table table-striped table-bordered">

    @{
        foreach (var consoleMessage in Model.ConsoleMessages)
        {
            <tr>
                <td>
                    <h4>@consoleMessage.Date.ToShortTimeString() - @consoleMessage.ConsoleMsg</h4>
                </td>
            </tr>
        }
    }

    </table>